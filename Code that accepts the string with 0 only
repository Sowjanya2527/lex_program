%{ 
%} 

%s A DEAD 


%% 
<INITIAL>0 BEGIN A; 
<INITIAL>[^0\n] BEGIN DEAD; 
<INITIAL>\n BEGIN INITIAL; {printf("Not Accepted\n");} 


<A>0 BEGIN A; 
<A>[^0\n] BEGIN DEAD; 
<A>\n BEGIN INITIAL; {printf("Accepted\n");} 


<DEAD>[^\n] BEGIN DEAD; 
<DEAD>\n BEGIN INITIAL; {printf("Invalid\n");} 

%% 

	

int yywrap() 
{ 
return 1; 
} 


int main() 
{ 
printf("Enter String\n"); 

yylex(); 
return 0; 
}
